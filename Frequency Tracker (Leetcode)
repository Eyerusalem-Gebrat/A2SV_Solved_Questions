from collections import defaultdict
class FrequencyTracker:

    def __init__(self):
        self.numsFreq = defaultdict(int)
        self.freq = defaultdict(int)

    def add(self, number: int) -> None:
        if self.numsFreq[number] in self.freq:
            self.freq[self.numsFreq[number]] -= 1

        self.numsFreq[number] += 1
        self.freq[self.numsFreq[number]] += 1
        
    def deleteOne(self, number: int) -> None:
        if (number in self.numsFreq) and (self.numsFreq[number] != 0):
            self.freq[self.numsFreq[number]] -= 1
            self.numsFreq[number] -= 1

            if (self.numsFreq[number] != 0):
                self.freq[self.numsFreq[number]] += 1
           
    def hasFrequency(self, frequency: int) -> bool:
        if (frequency in self.freq) and (self.freq[frequency] > 0):
            return True
        else: return False

# Your FrequencyTracker object will be instantiated and called as such:
# obj = FrequencyTracker()
# obj.add(number)
# obj.deleteOne(number)
# param_3 = obj.hasFrequency(frequency)
