class Solution:
    def isPalindrome(self, x: int) -> bool:
        if x < 0: return False
        if 0 <= x < 10: return True
        temp = x
        palindrome = 0
        while temp != 0:
            temp2 = temp % 10
            palindrome = palindrome * 10 + temp2
            temp //= 10
            
        return x == palindrome
