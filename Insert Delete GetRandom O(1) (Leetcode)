from random import randint
class RandomizedSet:

    def __init__(self):
        self.my_set = set()

    def insert(self, val: int) -> bool:
        if val not in self.my_set:
            self.my_set.add(val)
            return True
        return False

    def remove(self, val: int) -> bool:
        if val in self.my_set:
            self.my_set.remove(val)
            return True
        return False

    def getRandom(self) -> int:
        if len(self.my_set) == 1:
            return next(iter(self.my_set))
        else:
            my_list = list(self.my_set)
            random_index = randint(0, len(my_list) - 1)
            return my_list[random_index]


# Your RandomizedSet object will be instantiated and called as such:
# obj = RandomizedSet()
# param_1 = obj.insert(val)
# param_2 = obj.remove(val)
# param_3 = obj.getRandom()
